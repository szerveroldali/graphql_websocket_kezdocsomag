    "description": "Kezdőcsomag a GraphQL & Websocket zárthelyihez",
    "scripts": {
        "gql:dev": "cd graphql && npx nodemon server.js",
        "gql:db": "cd graphql && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
        "gql:test": "cd graphql && node test/tester.js",
        "ws:dev": "cd websocket && npx nodemon server.js",
        "ws:test": "cd websocket && node test/tester.js",
        "zip": "node zip.js",
        "prettier": "npx prettier --write \"**/*.{js,json,graphql,gql}\""
    },
